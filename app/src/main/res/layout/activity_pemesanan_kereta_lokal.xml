<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_pemesanan_kereta_lokal"
    android:layout_width="match_parent"
    android:layout_height="800dp"
    android:background="@drawable/container_pemesanan_kereta_lokal105">

    <FrameLayout
        android:id="@+id/container_frame4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top|left"
        android:background="@drawable/container_frame102">

        <TextView
            android:id="@+id/text_kereta_lokal"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_gravity="top|left"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="42dp"
            android:layout_marginBottom="-1dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Kereta Lokal"
            android:textColor="#FF000000"
            android:textSize="15dp"
            tools:ignore="TextSizeCheck" />

        <ImageView
            android:id="@+id/image_union"
            android:layout_width="20dp"
            android:layout_height="17dp"
            android:layout_marginLeft="23dp"
            android:layout_marginTop="44dp"
            android:layout_marginRight="317dp"
            android:layout_marginBottom="257dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_union112" />
    </FrameLayout>

    <ImageView
        android:id="@+id/image_arrow_left_normal"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="61dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        android:src="@drawable/image_arrow_left_normal" />

    <ImageView
        android:id="@+id/image_rectangle"
        android:layout_width="279dp"
        android:layout_height="417dp"
        android:layout_gravity="top|left"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="159dp"
        android:background="@drawable/image_rectangle108"
        android:scaleType="centerInside" />

    <FrameLayout
        android:id="@+id/container_frame"
        android:layout_width="230dp"
        android:layout_height="62dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="432dp"
        android:layout_gravity="top|left"
        android:background="@drawable/container_frame100">

        <LinearLayout
            android:id="@+id/container_frame1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="141dp"
            android:layout_marginBottom="-0.5dp"
            android:layout_gravity="top|left"
            android:clipToPadding="false"
            android:gravity="left|top"
            android:orientation="horizontal"
            android:paddingBottom="0.5dp"
            android:paddingTop="4.5dp">

            <TextView
                android:id="@+id/text_orang"
                android:layout_width="wrap_content"
                android:layout_height="17dp"
                android:layout_marginBottom="-0.5dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="3 Orang"
                android:textColor="#FF000000"
                android:textSize="13dp" />

            <TextView
                android:id="@+id/text_rp"
                android:layout_width="wrap_content"
                android:layout_height="17dp"
                android:layout_marginLeft="164dp"
                android:layout_marginBottom="-0.5dp"
                android:gravity="right|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Rp 000000;"
                android:textColor="#FFDCBD5E"
                android:textSize="13dp" />
        </LinearLayout>

        <ImageView
            android:id="@+id/image_seat_passenger"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="18dp"
            android:layout_gravity="top|left"
            android:scaleType="centerInside"
            android:src="@drawable/image_seat_passenger" />

        <TextView
            android:id="@+id/text_penumpang"
            android:layout_width="wrap_content"
            android:layout_height="9dp"
            android:layout_gravity="top|left"
            android:layout_marginLeft="52dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="-1dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Penumpang"
            android:textColor="#FF000000"
            android:textSize="6dp"
            tools:ignore="TextSizeCheck" />

        <TextView
            android:id="@+id/text_dewasa"
            android:layout_width="wrap_content"
            android:layout_height="14dp"
            android:layout_gravity="top|left"
            android:layout_marginLeft="52dp"
            android:layout_marginTop="27dp"
            android:layout_marginBottom="-1dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="1 Dewasa"
            android:textColor="#FF000000"
            android:textSize="10dp"
            tools:ignore="TextSizeCheck" />
    </FrameLayout>

    <Button
        android:id="@+id/cari_kereta"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_gravity="top|left"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="500dp"
        android:background="@drawable/button_rounded_corner"
        android:padding="16dp"
        android:text="Cari Kereta"
        android:textSize="15dp"
        tools:ignore="TextSizeCheck" />

    <FrameLayout
        android:id="@+id/arrival"
        android:layout_width="230dp"
        android:layout_height="62dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="275dp"
        android:layout_gravity="top|left"
        android:background="@drawable/container_frame101">

        <TextView
            android:id="@+id/text_ke"
            android:layout_width="wrap_content"
            android:layout_height="9dp"
            android:layout_gravity="top|left"
            android:layout_marginLeft="52dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="-1dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Ke"
            android:textColor="#FF000000"
            android:textSize="6dp"
            tools:ignore="TextSizeCheck" />

        <TextView
            android:id="@+id/text_ke1"
            android:layout_width="wrap_content"
            android:layout_height="14dp"
            android:layout_gravity="top|left"
            android:layout_marginLeft="52dp"
            android:layout_marginTop="27dp"
            android:layout_marginBottom="-1dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Ke"
            android:textColor="#FF000000"
            android:textSize="10dp"
            tools:ignore="TextSizeCheck" />

        <ImageView
            android:id="@+id/image_train_line"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_marginLeft="19dp"
            android:layout_marginTop="21dp"
            android:layout_gravity="top|left"
            android:scaleType="centerInside"
            android:src="@drawable/image_train_line" />
    </FrameLayout>

    <DatePicker
        android:id="@+id/container_frame5"
        android:layout_width="230dp"
        android:layout_height="62dp"
        android:layout_gravity="top|left"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="358dp"
        android:scaleType="centerInside"
        android:src="@drawable/container_frame103" />

    <FrameLayout
        android:id="@+id/departure"
        android:layout_width="230dp"
        android:layout_height="62dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="213dp"
        android:layout_gravity="top|left"
        android:background="@drawable/container_frame104">

        <LinearLayout
            android:id="@+id/container_frame8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="141dp"
            android:layout_marginBottom="-0.5dp"
            android:layout_gravity="top|left"
            android:clipToPadding="false"
            android:gravity="left|top"
            android:orientation="horizontal"
            android:paddingBottom="0.5dp"
            android:paddingTop="4.5dp"/>

        <TextView
            android:id="@+id/text_dari"
            android:layout_width="wrap_content"
            android:layout_height="9dp"
            android:layout_gravity="top|left"
            android:layout_marginLeft="52dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="-1dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Dari"
            android:textColor="#FF000000"
            android:textSize="6dp"
            tools:ignore="TextSizeCheck" />

        <TextView
            android:id="@+id/text_dari1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="top|left"
            android:layout_marginLeft="52dp"
            android:layout_marginTop="27dp"
            android:layout_marginBottom="-1dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Dari"
            android:textColor="#FF000000"
            android:textSize="10dp"
            tools:ignore="TextSizeCheck" />

        <ImageView
            android:id="@+id/image_train_line1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="19dp"
            android:layout_gravity="top|left"
            android:scaleType="centerInside"
            android:src="@drawable/image_train_line1" />
    </FrameLayout>

    <ImageView
        android:id="@+id/image_arrow_up_linear"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginLeft="259dp"
        android:layout_marginTop="252dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside" />

    <ImageView
        android:id="@+id/image_frame"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="243dp"
        android:layout_marginTop="260dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        android:src="@drawable/image_frame" />

</FrameLayout>
