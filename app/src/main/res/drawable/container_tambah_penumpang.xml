<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_tambah_penumpang"
    android:layout_width="match_parent"
    android:layout_height="800dp"
    android:gravity="left|top"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/container_frame"
        android:layout_width="360dp"
        android:layout_height="103dp"
        android:layout_gravity="left"
        android:background="@drawable/container_frame">

        <FrameLayout
            android:id="@+id/container_status_bar"
            android:layout_width="360dp"
            android:layout_height="43dp"
            android:layout_gravity="top|left">

            <ImageView
                android:id="@+id/image_bg"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:background="@drawable/image_bg1"
                android:scaleType="centerCrop" />

            <ImageView
                android:id="@+id/image_time"
                android:layout_width="54dp"
                android:layout_height="18.48dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="290dp"
                android:layout_marginTop="12.32dp"
                android:layout_marginBottom="12.21dp"
                android:scaleType="centerInside"
                android:src="@drawable/image_time1" />

            <ImageView
                android:id="@+id/image_alarm"
                android:layout_width="18dp"
                android:layout_height="18.48dp"
                android:layout_marginLeft="242dp"
                android:layout_marginRight="100dp"
                android:layout_marginTop="12.32dp"
                android:layout_marginBottom="12.21dp"
                android:scaleType="centerInside"
                android:src="@drawable/image_alarm1" />

            <ImageView
                android:id="@+id/image_signal"
                android:layout_width="18dp"
                android:layout_height="18.48dp"
                android:layout_marginLeft="305dp"
                android:layout_marginRight="37dp"
                android:layout_marginTop="12.32dp"
                android:layout_marginBottom="12.21dp"
                android:scaleType="centerInside"
                android:src="@drawable/image_signal1" />

            <ImageView
                android:id="@+id/image_wifi"
                android:layout_width="18dp"
                android:layout_height="18.48dp"
                android:layout_marginLeft="284dp"
                android:layout_marginRight="58dp"
                android:layout_marginTop="12.32dp"
                android:layout_marginBottom="12.21dp"
                android:scaleType="centerInside"
                android:src="@drawable/image_wifi1" />

            <ImageView
                android:id="@+id/image_bluetooth"
                android:layout_width="18dp"
                android:layout_height="18.48dp"
                android:layout_marginLeft="263dp"
                android:layout_marginRight="79dp"
                android:layout_marginTop="12.32dp"
                android:layout_marginBottom="12.21dp"
                android:scaleType="centerInside"
                android:src="@drawable/image_bluetooth1" />

            <ImageView
                android:id="@+id/image_battery"
                android:layout_width="18dp"
                android:layout_height="18.48dp"
                android:layout_marginLeft="326dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="12.32dp"
                android:layout_marginBottom="12.21dp"
                android:scaleType="centerInside"
                android:src="@drawable/image_battery1" />
        </FrameLayout>

        <TextView
            android:id="@+id/text_tambah_penumpang"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginRight="0.5dp"
            android:layout_marginTop="55.5dp"
            android:layout_marginBottom="-0.5dp"
            android:layout_gravity="center_horizontal"
            android:gravity="center_horizontal|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Tambah Penumpang"
            android:textColor="#FF000000"
            android:textSize="20dp" />

        <ImageView
            android:id="@+id/image_icon_left"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="316dp"
            android:layout_marginTop="56dp"
            android:layout_marginBottom="23dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_icon_left1" />
    </FrameLayout>

    <TextView
        android:id="@+id/text_jenis_identitas"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="15.5dp"
        android:layout_marginBottom="-0.5dp"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Jenis Identitas"
        android:textColor="#FF000000"
        android:textSize="13dp" />

    <FrameLayout
        android:id="@+id/container_frame3"
        android:layout_width="280dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="8dp">

        <LinearLayout
            android:id="@+id/container_form"
            android:layout_width="280dp"
            android:layout_height="wrap_content"
            android:layout_gravity="top|left"
            android:background="@drawable/container_form"
            android:clipToPadding="false"
            android:gravity="left|center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="11.5dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="11.5dp">

            <TextView
                android:id="@+id/text_nama_identitas"
                android:layout_width="wrap_content"
                android:layout_height="17dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="KTP/SIM/Paspor"
                android:textColor="#FFB3B4B3"
                android:textSize="13dp" />
        </LinearLayout>

        <ImageView
            android:id="@+id/image_vector"
            android:layout_width="9dp"
            android:layout_height="6dp"
            android:layout_marginLeft="253dp"
            android:layout_marginRight="18dp"
            android:layout_marginTop="19dp"
            android:layout_marginBottom="15dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_vector2" />
    </FrameLayout>

    <TextView
        android:id="@+id/text_nomor_identitas"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="14.5dp"
        android:layout_marginBottom="-0.5dp"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Nomor Identitas"
        android:textColor="#FF000000"
        android:textSize="13dp" />

    <LinearLayout
        android:id="@+id/container_form1"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/container_form1"
        android:clipToPadding="false"
        android:gravity="left|center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="11.5dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="11.5dp">

        <TextView
            android:id="@+id/text_nama_identitas1"
            android:layout_width="wrap_content"
            android:layout_height="17dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Nomor  Identitas"
            android:textColor="#FFB3B4B3"
            android:textSize="13dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/text_nama_lengkap"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="14.5dp"
        android:layout_marginBottom="-0.5dp"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Nama Lengkap "
        android:textColor="#FF000000"
        android:textSize="13dp" />

    <LinearLayout
        android:id="@+id/container_form2"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/container_form2"
        android:clipToPadding="false"
        android:gravity="left|center_vertical"
        android:orientation="horizontal"
        android:paddingBottom="11.5dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="11.5dp">

        <TextView
            android:id="@+id/text_nama"
            android:layout_width="wrap_content"
            android:layout_height="17dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Nama"
            android:textColor="#FFB3B4B3"
            android:textSize="13dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/text_tipe_penumpang"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="14.5dp"
        android:layout_marginBottom="-0.5dp"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Tipe Penumpang"
        android:textColor="#FF000000"
        android:textSize="13dp" />

    <FrameLayout
        android:id="@+id/container_frame7"
        android:layout_width="280dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp">

        <LinearLayout
            android:id="@+id/container_form3"
            android:layout_width="280dp"
            android:layout_height="wrap_content"
            android:layout_gravity="top|left"
            android:background="@drawable/container_form3"
            android:clipToPadding="false"
            android:gravity="left|center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="11.5dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="11.5dp">

            <TextView
                android:id="@+id/text_tipe_penumpang_detail"
                android:layout_width="wrap_content"
                android:layout_height="17dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Dewasa/Anak - Anak"
                android:textColor="#FFB3B4B3"
                android:textSize="13dp" />
        </LinearLayout>

        <ImageView
            android:id="@+id/image_vector1"
            android:layout_width="9dp"
            android:layout_height="6dp"
            android:layout_marginLeft="253dp"
            android:layout_marginRight="18dp"
            android:layout_marginTop="19dp"
            android:layout_marginBottom="15dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_vector1" />
    </FrameLayout>

    <LinearLayout
        android:id="@+id/container_button_component"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="292dp"
        android:layout_gravity="left"
        android:background="@drawable/container_button_component1"
        android:clipToPadding="false"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="15.5dp"
        android:paddingTop="15.5dp">

        <TextView
            android:id="@+id/text_simpan"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:gravity="center_horizontal|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Simpan"
            android:textColor="#FFFDFCFF"
            android:textSize="20dp" />
    </LinearLayout>
</LinearLayout>
